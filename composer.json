{
  "name": "philippgrashoff/pmratk",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config":{
    "platform":{
      "php":"7.3"
    }
  },
  "require":{
    "philippgrashoff/mtomforatk": "master",
    "philippgrashoff/secondarymodelforatk": "master",
    "philippgrashoff/traitsforatkdata": "master",
    "philippgrashoff/notificationforatk": "master",
    "philippgrashoff/cachedvalueforatk": "master",
    "philippgrashoff/settingsforatk": "master",
    "philippgrashoff/auditforatk": "master",
    "atk4/ui": "2.3.0",
    "atk4/data": "2.3.0",
    "atk4/dsql": "2.3.0",
    "atk4/core": "2.3.0",
    "atk4/login": "2.3.0",
    "atk4/schema": "2.3.0",
    "atk4/api": "0.3",
    "php-curl-class/php-curl-class": "*",
    "phpmailer/phpmailer": ">6",
    "league/period": ">4.0",
    "zendframework/zend-mail": "^2.10",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-sodium": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-imap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">8",
    "php-webdriver/webdriver": "^1.8.2",
    "ext-http": "*"
  },
  "autoload":{
    "psr-4":{
      "PMRAtk\\":"src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PMRAtk\\tests\\": "tests"
    }
  }
}

{
  "name": "philippgrashoff/pmratk",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config":{
    "platform":{
      "php":"7.2"
    }
  },
  "require":{
    "atk4/ui": ">1.7.0",
    "atk4/data": "dev-develop",
    "atk4/dsql": "dev-develop as 1.2.6",
    "atk4/core": "dev-develop",
    "atk4/login": "dev-develop",
    "atk4/schema": "1.1.6",
    "atk4/api": "dev-develop",
    "php-curl-class/php-curl-class": "*",
    "phpmailer/phpmailer": ">6",
    "league/period": ">4.0",
    "zendframework/zend-mail": "^2.10"
  },
  "require-dev": {
    "phpunit/phpunit": ">8",
    "facebook/webdriver": "dev-community"
  },
  "autoload":{
    "psr-4":{
      "PMRAtk\\":"src/",
      "PMRAtk\\tests\\":"tests/"
    }
  }
}
